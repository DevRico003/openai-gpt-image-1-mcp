# The transport for the MCP server - either 'sse' or 'stdio' (defaults to SSE if left empty)
TRANSPORT=

# Host to bind to if using sse as the transport (leave empty if using stdio)
HOST=

# Port to listen on if using sse as the transport (leave empty if using stdio)
PORT=

# Public URL where the server can be reached (default is http://HOST:PORT)
# Change this when your server is behind a proxy or has a different public address
PUBLIC_URL=

# Your OpenAI API key (required)
OPENAI_API_KEY=

# Optional settings for image generation
# MODEL_NAME=gpt-image-1
# DEFAULT_IMAGE_SIZE=auto
# DEFAULT_IMAGE_QUALITY=auto